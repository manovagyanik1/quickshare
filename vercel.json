{
  "version": 2,
  "routes": [
    {
      "src": "/auth-callback",
      "dest": "/"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
} 